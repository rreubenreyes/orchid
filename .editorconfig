[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.md]
charset = unset
end_of_line = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset

[Makefile]
indent_style = tab

[*.go]
indent_style = tab
